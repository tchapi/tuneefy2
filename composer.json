{
    "name": "tchapi/tuneefy",
    "description": "tuneefy is a unified way to share music with your friends, over various online music services",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "tchapi",
            "email": "regbasket+github@gmail.com",
            "homepage": "https://tchap.me"
        }
    ],
    "support": {
        "email": "team@tuneefy.com",
        "issues": "https://github.com/tchapi/tuneefy2/issues"
    },
    "autoload": {
        "psr-4": { "": "src/" }
    },
    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/tchapi/slim-oauth2-http"
      },
      {
          "type": "vcs",
          "url": "https://github.com/tchapi/slim-oauth2-routes"
      },
      {
          "type": "vcs",
          "url": "https://github.com/tchapi/slim-oauth2-middleware"
      }
    ],
    "config": {
      "preferred-install": {
          "*": "dist"
      },
      "sort-packages": true,
      "platform": {
          "php": "8.1.27"
      }
  },
    "require": {
        "php" : "^7.4 || ^8.0",
        "symfony/yaml": "^5.4",
        "slim/slim": "^4.0",
        "akrabat/rka-content-type-renderer": "^2.0",
        "chadicus/slim-oauth2-http": "dev-next",
        "chadicus/slim-oauth2-middleware": "dev-next",
        "chadicus/slim-oauth2-routes": "dev-next",
        "symfony/translation": "^5.4",
        "tuupola/slim-basic-auth": "^3.3",
        "dflydev/fig-cookies": "^3.0",
        "bryanjhv/slim-session": "^4.1.2",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "slim/twig-view": "^3.2",
        "symfony/twig-bridge": "^5.4",
        "slim/psr7": "^1.5",
        "symfony/mailer": "^5.4"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.2",
        "friendsofphp/php-cs-fixer": "^3.2",
        "softcreatr/jsonpath": "^0.7.5"
    }
}
